{
  "dependencies": {
    "fluidbox": "^2.0.5",
    "turbolinks": "^5.2.0",
    "yarn": "^1.22.17"
  },
  "scripts": {
    "develop": "bundle exec jekyll clean && run-p develop:{webpack,jekyll}",
    "develop:jekyll": "sleep 5 && bundle exec jekyll serve --watch",
    "develop:webpack": "npm rebuild node-sass && webpack --mode development --watch",
    "build": "run-s build:webpack build:jekyll purgecss",
    "build:jekyll": "JEKYLL_ENV=production bundle exec jekyll build",
    "build:webpack": "webpack --mode production"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "cssnano": "^5.0.9",
    "imagesloaded": "^4.1.4",
    "jquery": "^3.6.0",
    "jquery-bridget": "^3.0.0",
    "masonry-layout": "^4.2.2",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss-loader": "^6.2.0",
    "postcss-strip-inline-comments": "^0.1.5",
    "sass-loader": "^12.3.0",
    "scss-reset": "^1.2.2",
    "style-loader": "^3.3.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  }
}
